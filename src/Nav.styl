#Main-Nav {
    margin: auto;
    display: flex;
    justify-content: space-around;
    width: 40rem;
    max-width: 100vw;
    padding: 0;
    padding-top: 0.5rem;
    padding-bottom: 0.5rem;
}
#Main-Nav-Container {
    /*
    display: flex;
    justify-content: space-between;
    */
    color: DIM_DARK_GOLD;
    align-items: center;
    margin: 0;
    padding: 0;



    .version-language {
        display: flex
        justify-content: space-between;
        align-items: center;
        text-align: left;
        span {
            margin: 0.3rem;
            display: flex
            align-items: center;
        }
    }
}

.about-link, .github-link {
    color: DIM_DARK_GOLD !important;
    padding: 0;
    &:hover {
        background-color: transparent !important;
        border: 1px solid transparent;
    }
}
